//rule
if import != false
  @import 'rule'

//stage
#guide
  width 100%
  height 32px
  background-color #333
  color #fff
  line-height @height

#guide-left
  float left

#guide-right
  float right

  .item
    float left
    margin 0
    padding 0 16px
    color #eee
    cursor pointer

    &:hover
      color #fff
      background-color #222

#logo-guide
  font-size 14px
  font-weight bold
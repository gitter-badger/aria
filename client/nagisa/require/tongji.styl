//rule
if import != false
  @import 'rule'

//baidu tongji
#block-baidu-tongji
  @extend .inline-block
  position relative
  top 2px
  margin 0 0 0 4px
  padding 0
  width 12px
  height 12px

  a
    display block
    margin 0
    padding 0
    width 100%
    height 100%
    border-radius 2px
    background url("data:image/png;base64,") left top no-repeat

  img
    display none